{
  "name": "issue",
  "displayName": "issue",
  "description": "issue helper",
  "version": "0.0.1",
  "publisher": "octref",
  "engines": {
    "vscode": "^1.18.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index",
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.previewPerfIssue",
        "title": "Preview Perf Issue"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p .",
    "watch": "tsc -w -p .",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/node": "^8.0.51",
    "@types/opn": "^5.1.0",
    "prettier": "^1.8.2",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.9"
  },
  "dependencies": {
    "opn": "^5.1.0"
  }
}
